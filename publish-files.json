[{
        "from": "node_modules/angular-material/angular-material.min.css",
        "to": "css/angular-material.min.css"
    }, {
        "from": "node_modules/jquery/dist/jquery.min.js",
        "to": "jquery.min.js"
    },
    { "from": "src/index.html", "to": "index.html" }, {
        "from": {
            "glob": "./src/**/*.view.html"
        },
        "to": "./",
        "flatten": true
    }, {
        "from": {
            "glob": "./src/img/*"
        },
        "to": "./img",
        "flatten": true
    }
]
